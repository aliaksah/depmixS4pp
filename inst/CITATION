citHeader("To cite depmixS4pp in publications use:")

citEntry(entry = "Article",
  title        = "An Adaptive Simulated Annealing EM Algorithm for Inference on Non-homogeneous Hidden Markov Models",
  author       = personList(as.person("Aliaksandr Hubin"))      
  journal      = "International Conference on Artificial Intelligence, Information Processing and Cloud Computing 2019"
  year         = "2019",
  pages        = "1--9",
  url          = "http://www.aiipcc.org/",

  textVersion  =
  paste("Aliaksandr Hubin (2019).",
        "An Adaptive Simulated Annealing EM Algorithm for Inference on Non-homogeneous Hidden Markov Models",
        "AIPCC2019",
        "URL http://www.aiipcc.org/")
)

